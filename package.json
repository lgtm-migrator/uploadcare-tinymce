{
  "name": "uploadcare-tinymce",
  "version": "2.1.1",
  "widgetVersion": "3.x",
  "description": "This is Uploadcare plugin for TinyMCE text editor.",
  "main": "uploadcare.tinymce.js",
  "scripts": {
    "dev": "rollup -c -w",
    "lint": "eslint .",
    "precommit": "npm run lint",
    "serve": "serve",
    "release": "release-it",
    "deploy:demo": "node scripts/deploy-demo.js",
    "deploy:release": "node scripts/deploy-release.js",
    "build": "npm run clean && rollup -c && npm run archive",
    "clean": "rimraf dist",
    "archive": "node scripts/archive-dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-tinymce.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-tinymce/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-tinymce#readme",
  "devDependencies": {
    "archiver": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-config-uploadcare": "^1.1.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "release-it": "^7.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-cp": "^0.1.3",
    "rollup-plugin-jscc": "^0.3.3",
    "rollup-plugin-license": "^0.6.0",
    "serve": "^6.5.6"
  }
}
